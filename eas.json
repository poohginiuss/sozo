{
  "cli": {
    "version": ">= 16.14.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "env": {
          "ANDROID_GRADLE_PLUGIN_VERSION": "8.5.0"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
